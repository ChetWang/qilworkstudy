<?xml version="1.0" encoding="UTF-8"?>
<NCI xmlns:nci="http://www.nci.com.cn">
	<!--台区属性：code 功能位置编码,name 名称，basevol 电压等级，company 公司代码，serviceStation 所属营业所代码，village 小区代码-->
	<Area code="5028-P-E400-0124" name="妙智桥台区" basevol="E"
		company="JXXZ" serviceStation="08" village="A10P">
		<DsTransformer code="111" name="配变" voltage="110kV"
			capacity="80KVA" />

		<!-- 线路属性： code 线路编码，name 名称，leadModel 导线规格，distance 总长度KM，from 开始杆，to 终止杆  -->
		<!-- startAngle 起始角度,middleTag 中间接入标记-->
		<!-- 1个台区有1到n个线路 -->
		<Lines>
			<!-- 每条线路提供起始角，角度以向右水平方向逆时针旋转计算，单位角度，线路起始角startAngle是绝对转角，针对台区图坐标 -->
			<Line code="17A03C36-4541-1DC2-24C3-670807A62880"
				name="妙智桥干线" leadModel="LJ-35" from="1#" to="13#" distance="2"
				startAngle="270" middleTag="true">
				<Poles>
					<!-- 杆塔属性：code 杆塔编码，name 名称，model 杆型，kind 杆塔类型, height 高度M，distance 档距,angle 转角（转角是相对前两个杆的连线形成的相对夹角，顺时针为负，逆时针为正，正负180度） -->
					<!--  contactTag 接入点标记， contactAngle 接入角度（相对角度，接入点前杆塔和接入点杆塔连线相对于接出设备的转角） -->
					<!-- angle 杆塔转角是以前一杆塔延长线的夹角，顺时针为负，逆时针为正，正负180度，单位角度 -->
					<!-- Pole上的distance表示的是该杆塔与前一个杆塔之间的档距，单位1m=1px -->
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26901"
						name="1#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26902"
						name="2#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26903"
						name="3#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" angle="10" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26904"
						name="4#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" angle="10" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26905"
						name="5#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26906"
						name="6#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26907"
						name="7#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" angle="-90" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26908"
						name="8#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" angle="90" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26909"
						name="9#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" angle="0" contactTag="true" contactAngle="0" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26910"
						name="10#" model="水泥杆" kind="转角杆" opType="单杆单回" height="30"
						distance="30" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26911"
						name="11#" model="水泥杆" kind="转角杆" opType="单杆单回" height="30"
						distance="30" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26912"
						name="12#" model="水泥杆" kind="转角杆" opType="单杆单回" height="30"
						distance="30" angle="-90" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26913"
						name="13#" model="水泥杆" kind="转角杆" opType="单杆单回" height="30"
						distance="30" />
				</Poles>
			</Line>

			<!-- 支线线属性： code 线路编码，name 名称，leadModel 导线规格，length 长度KM，pline 父线路id,startPole 分支起始杆（父线路杆）-->
			<!--  from 开始杆，to 终止杆 ，contactTag 接入点标记， contactAngle 接入角度 ，middleTag 中间接入标记（正常是T接支线，标记为true表示十字接）-->
			<!--1个主线下有1到n个支线，1个支线下有1到n个支线，支线和主线中的杆塔集合类似-->
			<Line code="2E5DC0CD-5D03-6B62-462E-1A0B22B1FD85"
				name="沈家分线" leadModel="LJ-35"
				pline="17A03C36-4541-1DC2-24C3-670807A62880"
				startPole="FBB5CCCD-C0E3-0ABA-29EE-287828A26913" from="1#" to="4#"
				distance="0.2" startAngle="300" middleTag="true">
				<Poles>
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26955"
						name="1#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" contactTag="true"/>
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26956"
						name="2#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26957"
						name="3#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" />
					<Pole code="FBB5CCCD-C0E3-0ABA-29EE-287828A26958"
						name="4#" model="水泥杆" kind="直线杆" opType="单杆单回" height="30"
						distance="30" />
				</Poles>
			</Line>
		</Lines>

	</Area>
</NCI>
