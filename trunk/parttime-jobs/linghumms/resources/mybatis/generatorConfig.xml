<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
  <properties url="file:////D:/documents/workspace-4.2/ama-3.0/resources/mybatis/generator.properties" />
  <context id="psg-service-ingegration" targetRuntime="MyBatis3">
  	<plugin type="org.mybatis.generator.plugins.MapperConfigPlugin">
  	    <property name="targetPackage" value="mybatis"/>
  		<property name="targetProject" value="${targetResources}"/>
  	</plugin>
  	
  	<plugin type="org.mybatis.generator.plugins.SerializablePlugin">
  		<property name="targetPackage" value="${serializablePackage}"/>
  		<property name="targetProject" value="${targetProject}"/>
  	</plugin>
  	

  	<plugin type="org.virbraligo.db.mybatis.OraclePagingPlugin"></plugin>
  
    <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
    <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
    
    <!-- 
    <plugin type="com.db.espear.psg.controlm.sla.dashboard.shared.LowercaseColumnNamesPlugin" /> -->
    <commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
    
    <jdbcConnection driverClass="${db_driverClass}" 
    	connectionURL="${db_connectionURL}" 
    	userId="${db_userid}" 
    	password="${db_password}">
    </jdbcConnection>
    
    <javaTypeResolver type="mybatis.AMAJavaTypeResolver" >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="${modelPackage}" targetProject="${targetProject}">
      <property name="enableSubPackages" value="false" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="${sqlMapperXMLPackage}"  targetProject="${targetProject}">
      <property name="enableSubPackages" value="false" />
    </sqlMapGenerator>

    <javaClientGenerator type="MIXEDMAPPER" targetPackage="${daoMapperPackage}"  targetProject="${targetProject}">
      <property name="enableSubPackages" value="false" />
    </javaClientGenerator>

  <!-- 增加或修改在下面加上 /先看 README -->   
  <table  schema="${schema}" tableName="AMA2_MANUAL_WORKAROUND" domainObjectName="ManualWorkaroundBean" />
  <!-- 
   <table  schema="${schema}" tableName="AMA2_USER" domainObjectName="User" /> 
   
   <table  schema="${schema}" tableName="AMA2_MW_RISK" domainObjectName="RiskQuestion" /> -->
  </context>
  
</generatorConfiguration>