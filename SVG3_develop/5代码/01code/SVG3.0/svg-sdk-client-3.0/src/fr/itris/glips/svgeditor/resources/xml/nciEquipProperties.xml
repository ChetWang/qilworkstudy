<?xml version="1.0" encoding="UTF-8"?>
<EquipProperties>
	<!--
		Properties数据为单个设备的具体属性集合；componentName为“dynamic”的组件是动态变化的combo值,如果code是“-1”，则不会作为一个属性参数
	-->
	<!--  equipType 表示设备的名称 -->
	<Properties equipType="变电所" value="substation">
		<Property name="站所编码" code="appcode" value="" autoCalculate="true"
			component="text" />
		<Property name="地区" code="district" value="" autoCalculate="false"
			component="combo" componentName="地区" />
		<Property name="输配变类型" code="spbType" value="" autoCalculate="false"
			component="combo" componentName="输配变类型" />
		<Property name="电压等级" code="voltageLevel" value=""
			autoCalculate="false" component="combo" componentName="电压等级" />
		<Property name="编号" code="id" value="" autoCalculate="false"
			component="text" />
		<Property name="站所名称" code="name" value="" autoCalculate="false"
			component="text" />
	</Properties>
	<Properties equipType="发电厂" value="powerStation">
		<Property name="发电厂编码" code="appcode" value="" autoCalculate="true"
			component="text" />
		<Property name="地区" code="district" value="" autoCalculate="false"
			component="combo" componentName="地区" />
		<Property name="输配变类型" code="spbType" value="" autoCalculate="false"
			component="combo" componentName="输配变类型" />
		<Property name="编号" code="id" value="" autoCalculate="false"
			component="text" />
		<Property name="发电厂名称" code="name" value="" autoCalculate="false"
			component="text" />
	</Properties>
	<Properties equipType="线路" value="powerLine">
		<Property name="线路编码" code="appcode" value="" autoCalculate="true"
			component="text" />
		<Property name="地区" code="district" value="" autoCalculate="false"
			component="combo" componentName="地区" />
		<Property name="输配变类型" code="spbType" value="" autoCalculate="false"
			component="combo" componentName="输配变类型" />
		<Property name="电压等级" code="voltageLevel" value=""
			autoCalculate="false" component="combo" componentName="电压等级" />
		<Property name="编号" code="id" value="" autoCalculate="false"
			component="text" />
		<Property name="线路名称" code="name" value="" autoCalculate="false"
			component="text" />
		<Property name="起始" code="start" value="" autoCalculate="true"
			component="text" />
		<Property name="终止" code="end" value="" autoCalculate="true"
			component="text" />
	</Properties>
</EquipProperties>